<?xml version="1.0"?>

<aeonwave>
 <sound frequency="220">
  <waveform src="sawtooth"/>
  <waveform src="sawtooth" processing="add" ratio="0.75" pitch="2.0"/>
 </sound>

 <emitter looping="true">
  <filter type="envelope">
   <slot n="0">
    <param n="0">1.0</param>
    <param n="1">10.0</param>
    <param n="2">1.0</param>
    <param n="3">1.0</param>
   </slot>
  </filter>
  <filter type="dynamic-gain" src="impulse">
   <slot n="0">
    <param n="0">0.0</param>
    <param n="1">1.0</param>
    <param n="2">1.0</param>
    <param n="3">0.0</param>
   </slot>
  </filter>
 </emitter>

 <audioframe>
  <effect type="reverb">
   <slot n="0">
    <param n="0">5002.9</param>
    <param n="1">0.0020</param>
    <param n="2">0.944</param>
    <param n="3">0.350</param>
   </slot>
<!--
   <slot n="1">
    <param n="0">200.0</param>
    <param n="1">200.0</param>
    <param n="2">200.0</param>
    <param n="3">0.500</param>
   </slot>
-->
   <slot n="2">
    <param n="0">0.0</param>
    <param n="1">0.2</param>
    <param n="2">1.0</param>
    <param n="3">0.0</param>
   </slot>
  </effect>
 </audioframe>

</aeonwave>
