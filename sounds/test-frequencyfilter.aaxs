<?xml version="1.0"?>

<aeonwave>
 <sound frequency="220">
  <waveform src="sawtooth"/>
  <waveform src="sawtooth" processing="add" ratio="0.75" pitch="2.0"/>
 </sound>

 <emitter looping="true">
  <filter type="frequency" src="inverse-timed|logarithmic">
   <slot n="0">
    <param n="0">50.0</param>
    <param n="1">0.75</param>
    <param n="2">0.0</param>
    <param n="3">3.0</param>
   </slot>
   <slot n="1">
    <param n="0">5000.0</param>
    <param n="1">0.0</param>
    <param n="2">0.0</param>
    <param n="3">0.01</param>
   </slot>
  </filter>
  <filter type="envelope">
   <slot n="0">
    <param n="0">1.0</param>
    <param n="1">10.0</param>
    <param n="2">1.0</param>
    <param n="3">1.0</param>
   </slot>
  </filter>
 </emitter>

</aeonwave>
